    public static void setRedstoneColors(float af[][])
    {
        if(af.length != 16)
        {
            throw new IllegalArgumentException("Must be 16 colors.");
        }
        for(int i = 0; i < af.length; i++)
        {
            if(af[i].length != 3)
            {
                throw new IllegalArgumentException("Must be 3 channels in a color.");
            }
        }

        redstoneColors = af;
    }

