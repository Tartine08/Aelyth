    public static float redstoneColors[][];

    static 
    {
        redstoneColors = new float[16][];
        for(int i = 0; i < redstoneColors.length; i++)
        {
            float f = (float)i / 15F;
            float f1 = f * 0.6F + 0.4F;
            if(i == 0)
            {
                f = 0.0F;
            }
            float f2 = f * f * 0.7F - 0.5F;
            float f3 = f * f * 0.6F - 0.7F;
            if(f2 < 0.0F)
            {
                f2 = 0.0F;
            }
            if(f3 < 0.0F)
            {
                f3 = 0.0F;
            }
            redstoneColors[i] = (new float[] {
                f1, f2, f3
            });
        }

    }
